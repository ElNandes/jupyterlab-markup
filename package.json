{
  "name": "@agoose77/jupyterlab-markup",
  "version": "0.1.5",
  "description": "Additional markdown rendering support in markdown.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/agoose77/jupyterlab-markup",
  "bugs": {
    "url": "https://github.com/agoose77/jupyterlab-markup/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Angus Hollands",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/agoose77/jupyterlab-markup.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.1",
    "@jupyterlab/codemirror": "^1.0.0",
    "@jupyterlab/fileeditor": "^1.0.1",
    "@jupyterlab/markdownviewer": "^1.0.1",
    "@jupyterlab/rendermime": "^1.0.0",
    "@jupyterlab/rendermime-interfaces": "^1.3.0",
    "@phosphor/messaging": "^1.2.3",
    "markdown-it": "^8.4.2",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-diagrams": "0.0.6",
    "markdown-it-footnote": "^3.0.1"
  },
  "devDependencies": {
    "@types/codemirror": "~0.0.75",
    "@types/markdown-it": "^0.0.7",
    "@types/markdown-it-container": "^2.0.2",
    "rimraf": "^2.6.1",
    "typescript": "~3.1.1"
  },
  "jupyterlab": {
    "extension": true
  }
}
