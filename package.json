{
  "name": "@agoose77/jupyterlab-markup",
  "version": "0.2.1",
  "description": "Additional markdown rendering support in markdown.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/agoose77/jupyterlab-markup",
  "bugs": {
    "url": "https://github.com/agoose77/jupyterlab-markup/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Angus Hollands",
  "files": [
    "{src,lib}/**/*.{ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "LICENSE.txt"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/agoose77/jupyterlab-markup.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "lint": "prettier --list-different --write \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md,.yml}\""
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.2",
    "@jupyterlab/codemirror": "^2.0.2",
    "@jupyterlab/fileeditor": "^2.0.2",
    "@jupyterlab/markdownviewer": "^2.0.2",
    "@jupyterlab/rendermime": "^2.0.2",
    "@jupyterlab/rendermime-interfaces": "^2.0.1",
    "@lumino/messaging": "^1.3.3",
    "markdown-it": "^10.0.0",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-diagrams": "^0.1.2",
    "markdown-it-footnote": "^3.0.2",
    "prettier": "^2.1.2"
  },
  "devDependencies": {
    "@types/codemirror": "^0.0.74",
    "@types/markdown-it": "^0.0.9",
    "@types/markdown-it-container": "^2.0.2",
    "rimraf": "~3.0.0",
    "typescript": "~3.7.3"
  },
  "jupyterlab": {
    "extension": "lib/plugin.js"
  }
}
